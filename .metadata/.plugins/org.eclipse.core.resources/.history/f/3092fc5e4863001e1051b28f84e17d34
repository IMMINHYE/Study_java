package chapter4;

public class ifTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// p.100 if조건문 나 혼자 코딩
		int score = 84;
		if (score <= 100 && score >= 90) {
			System.out.println("A");
		}else if (score >= 80){
			System.out.println("B");
		}else if (score >= 70){
			System.out.println("C");
		}else if (score >= 60){
			System.out.println("D");
		}else{
			System.out.println("F");
		}
	}
}
